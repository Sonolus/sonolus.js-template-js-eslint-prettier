{
  "name": "sonolus.js-template-js-eslint-prettier",
  "version": "1.0.0-beta.0",
  "description": "Sonolus.js Template (JS + ESLint + Prettier)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "sonolus-cli --dev",
    "lint": "eslint --ext .js ./src",
    "lint-fix": "eslint --ext .js ./src --fix",
    "build": "sonolus-cli --build"
  },
  "devDependencies": {
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "sonolus-core": "~7.0.0",
    "sonolus.js": "~9.0.0"
  }
}
