{
  "name": "sonolus.js-template-js-eslint-prettier",
  "version": "1.0.0",
  "description": "Sonolus.js Template (JS + ESLint + Prettier)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "sonolus-cli --dev",
    "lint": "eslint --ext .js ./src",
    "lint-fix": "eslint --ext .js ./src --fix",
    "build": "sonolus-cli --build"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "sonolus-core": "~7.2.0",
    "sonolus.js": "~9.1.0"
  }
}
