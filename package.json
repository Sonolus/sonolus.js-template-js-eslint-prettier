{
  "name": "sonolus.js-template-js-eslint-prettier",
  "version": "1.4.1",
  "description": "Sonolus.js Template (JS + ESLint + Prettier)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:play": "sonolus-cli --dev ./play",
    "dev:watch": "sonolus-cli --dev ./watch",
    "dev:preview": "sonolus-cli --dev ./preview",
    "dev:tutorial": "sonolus-cli --dev ./tutorial",
    "format": "prettier . --write",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "build": "sonolus-cli --build ./play && sonolus-cli --build ./watch && sonolus-cli --build ./preview && sonolus-cli --build ./tutorial"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@sonolus/core": "~7.5.1",
    "@sonolus/sonolus.js": "~9.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4"
  }
}
