{
  "name": "sonolus.js-template-js-eslint-prettier",
  "version": "1.0.0",
  "description": "Sonolus.js Template (JS + ESLint + Prettier)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:play": "sonolus-cli --dev ./play/sonolus-cli.config.js",
    "dev:tutorial": "sonolus-cli --dev ./tutorial/sonolus-cli.config.js",
    "lint": "eslint --ext .js ./shared && eslint --ext .js ./play && eslint --ext .js ./tutorial",
    "lint-fix": "eslint --ext .js ./shared --fix && eslint --ext .js ./play --fix && eslint --ext .js ./tutorial --fix",
    "build": "sonolus-cli --build ./play/sonolus-cli.config.js && sonolus-cli --build ./tutorial/sonolus-cli.config.js"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "sonolus-core": "~7.2.0",
    "sonolus.js": "~9.1.0"
  }
}
