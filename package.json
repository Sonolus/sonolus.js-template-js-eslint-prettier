{
  "name": "sonolus.js-template-js-eslint-prettier",
  "version": "1.1.2",
  "description": "Sonolus.js Template (JS + ESLint + Prettier)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:play": "sonolus-cli --dev ./play",
    "dev:tutorial": "sonolus-cli --dev ./tutorial",
    "format": "prettier . --write",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "build": "sonolus-cli --build ./play && sonolus-cli --build ./tutorial"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "sonolus-core": "~7.3.0",
    "sonolus.js": "~9.2.0"
  }
}
